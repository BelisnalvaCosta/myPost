{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Depurador do Python: Arquivo Atual",
      "type": "debugpy",
      "request": "launch",
      "program": "${app.py}",
      "console": "integratedTerminal"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "c:/Projetos Pycharm-Python/myPost",
      "program": "c:/Projetos Pycharm-Python/myPost/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}